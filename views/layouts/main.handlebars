  {{/each}}

  {{#each scripts}}
  <script src={{this}} type="module"></script>
  {{/each}}
  <title>{{titre}}</title>
</head>

<body>

  {{> header}}
  <a href="/"></a>
  <main>
    {{{body}}}
  </main>
  {{> footer}}

</body>

</html>